<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
  <title>{{=it.wiki}}: {{=it.title}}</title>
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
  <link rel="stylesheet" href="templates/current/static/assets/css/normalize.css">
  <link rel="stylesheet" href="templates/current/static/assets/css/style.css">
  <link rel="stylesheet" href="templates/current/static/assets/css/content.css">

  <!-- Dynamic -->
  <link rel="stylesheet" href="templates/nouwiki_ui.css">
  <link rel="stylesheet" href="templates/current/dynamic/assets/css/prose-bright.css">
  <link rel="stylesheet" href="templates/current/dynamic/assets/css/ui.css">

  <!-- Global User Import -->
  {{ for(var i in it.global_import) { }}
  <link rel="import" href="{{=it.global_import[i]}}">
  {{ } }}

  <!-- Local User Import -->
  {{ for(var i in it.local_import) { }}
  <link rel="import" href="{{=it.local_import[i]}}">
  {{ } }}

  <!-- Global User CSS -->
  {{ for(var i in it.global_css) { }}
  <link rel="stylesheet" href="{{=it.global_css[i]}}">
  {{ } }}

  <!-- Local User CSS -->
  {{ for(var i in it.local_css) { }}
  <link rel="stylesheet" href="{{=it.local_css[i]}}">
  {{ } }}
  <script>
    var wiki = "{{=it.wiki}}";
  </script>
</head>
<body>
  <div id="controles" class="disabled">
    <a href="/"><button id="home">Home</button></a>
    <button id="edit">Edit</button>
    <button id="discard" class="edit">Discard</button>
    <button id="save" class="edit">Save</button>
    <button id="delete" class="edit">Delete Page</button>
    <div id="right_controles">
      <input id="grep_pages" type="text" placeholder="Search Pages" />
      <ul id="matches">
      </ul>
    </div>
  </div>
  <div id="content" class="markdown-body">
  {{=it.html}}
  </div>
  <div id="editor">
    <textarea></textarea>
    <div id="preview" class="markdown-body"></div>
  </div>

  <!-- Dynamic -->
  <script src="templates/nouwiki_ui.js"></script>
  <script src="templates/current/dynamic/assets/js/ui.js"></script>


  <!-- Global User JS -->
  {{ for(var i in it.global_js) { }}
  <script src="{{=it.global_js[i]}}"></script>
  {{ } }}

  <!-- Local User JS -->
  {{ for(var i in it.local_js) { }}
  <script src="{{=it.local_js[i]}}"></script>
  {{ } }}
</body>
</html>
